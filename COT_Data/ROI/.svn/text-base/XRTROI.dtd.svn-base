<?xml version="1.0" encoding="UTF-8"?>

<!ELEMENT ROI_TBL (modified*, name*, COT_info*, COT_slot*, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY, ROI_ENTRY)>

<!ELEMENT ROI_ENTRY (ROI_NUMBER, DESIGNATION, HCENTER, VCENTER, HSIZE, VSIZE)>
<!ELEMENT ROI_NUMBER (#PCDATA)>
<!ATTLIST ROI_NUMBER unit CDATA #IMPLIED>
<!ELEMENT DESIGNATION (#PCDATA)>
<!ATTLIST DESIGNATION unit CDATA #IMPLIED>
<!ELEMENT HCENTER (#PCDATA)>
<!ATTLIST HCENTER unit CDATA #IMPLIED>
<!ELEMENT VCENTER (#PCDATA)>
<!ATTLIST VCENTER unit CDATA #IMPLIED>
<!ELEMENT HSIZE (#PCDATA)>
<!ATTLIST HSIZE unit CDATA #IMPLIED>
<!ELEMENT VSIZE (#PCDATA)>
<!ATTLIST VSIZE unit CDATA #IMPLIED>

<!ELEMENT modified (#PCDATA)>
<!ATTLIST modified
	author CDATA #IMPLIED
	dateTime CDATA #IMPLIED
>

<!ELEMENT name (#PCDATA)>

<!ELEMENT COT_info (#PCDATA)>
<!ATTLIST COT_info
    deleted CDATA #IMPLIED
    lastUsed CDATA #IMPLIED
    version CDATA #IMPLIED
>
<!ELEMENT COT_slot (#PCDATA)>
