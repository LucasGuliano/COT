<?xml version="1.0" encoding="UTF-8"?>

<!ELEMENT CMP_TBL (modified*, name*, COT_info*, COT_slot*, CMP_ENTRY+)>

<!ELEMENT CMP_ENTRY (CMP_NUMBER, CMP_NAME, CMP_AVG)>
<!ELEMENT CMP_NUMBER (#PCDATA)>
<!ELEMENT CMP_NAME (#PCDATA)>
<!ELEMENT CMP_AVG (#PCDATA)>
<!ELEMENT modified (#PCDATA)>
<!ATTLIST modified
